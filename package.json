{
    "name": "n8n-nodes-article-extract",
    "version": "0.1.0",
    "description": "An n8n community node to extract article content from websites using Mozilla's Readability library",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "article",
        "extract",
        "readability",
        "mozilla"
    ],
    "license": "MIT",
    "homepage": "https://github.com/lukebtaylor/n8n-nodes-article-extract",
    "author": {
        "name": "Luke Taylor",
        "url": "https://github.com/lukebtaylor"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukebtaylor/n8n-nodes-article-extract.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "npx rimraf dist && tsc",
        "dev": "tsc --watch",
        "format": "prettier --write .",
        "lint": "eslint --ext .ts --quiet .",
        "test": "jest",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [],
        "nodes": [
            "dist/src/nodes/ArticleExtractor/ArticleExtractor.node.js"
        ]
    },
    "engines": {
        "n8n": ">=1.0.0"
    },
    "dependencies": {
        "@mozilla/readability": "^0.6.0",
        "jsdom": "^26.1.0",
        "n8n-workflow": "^1.82.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.15.30",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.4",
        "typescript": "^5.8.3"
    },
    "type": "commonjs"
}
